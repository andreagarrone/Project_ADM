Round4_Grappling_Standups_Landed float,
Round4_Grappling_Submissions_Attempts float,
Round4_Grappling_Takedowns_Attempts float
Round4_Grappling_Takedowns_Landed float,
Round5_Grappling_Reversals_Landed float,
Round5_Grappling_Standups_Landed float,
Round5_Grappling_Submissions_Attempts float,
Round5_Grappling_Takedowns_Attempts float
Round5_Grappling_Takedowns_Landed float,
PRIMARY KEY (ID, Fight_ID)
);

