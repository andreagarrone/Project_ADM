Round1_Strikes_Body Significant Strikes_Attempts float,
Round1_Strikes_Body Significant Strikes_Landed float,
Round1_Strikes_Body Total Strikes_Attempts float,
Round1_Strikes_Body Total Strikes_Landed float,
Round1_Strikes_Clinch Body Strikes_Attempts float,
Round1_Strikes_Clinch Body Strikes_Landed float,
Round1_Strikes_Clinch Head Strikes_Attempts float,
Round2_Grappling_Reversals_Landed float,
Round2_Grappling_Standups_Landed float,
Round2_Grappling_Submissions_Attempts float,
Round2_Grappling_Takedowns_Attempts float
Round2_Grappling_Takedowns_Landed float,
Round2_Strikes_Body Significant Strikes_Attempts float,
Round2_Strikes_Body Significant Strikes_Landed float,
Round2_Strikes_Body Total Strikes_Attempts float,
Round2_Strikes_Body Total Strikes_Landed float,
Round2_Strikes_Clinch Body Strikes_Attempts float,
Round2_Strikes_Clinch Body Strikes_Landed float,
Round2_Strikes_Clinch Head Strikes_Attempts float,
Round3_Grappling_Reversals_Landed float,
Round3_Grappling_Standups_Landed float,
Round3_Grappling_Submissions_Attempts float,
Round3_Grappling_Takedowns_Attempts float
Round3_Grappling_Takedowns_Landed float,
Round3_Strikes_Body Significant Strikes_Attempts float,
Round3_Strikes_Body Significant Strikes_Landed float,
Round3_Strikes_Body Total Strikes_Attempts float,
Round3_Strikes_Body Total Strikes_Landed float,
Round3_Strikes_Clinch Body Strikes_Attempts float,
Round3_Strikes_Clinch Body Strikes_Landed float,
Round3_Strikes_Clinch Head Strikes_Attempts float,
Round4_Grappling_Reversals_Landed float,
Round4_Grappling_Standups_Landed float,
Round4_Grappling_Submissions_Attempts float,
Round4_Grappling_Takedowns_Attempts float
Round4_Grappling_Takedowns_Landed float,
Round4_Strikes_Body Significant Strikes_Attempts float,
Round4_Strikes_Body Significant Strikes_Landed float,
Round4_Strikes_Body Total Strikes_Attempts float,
Round4_Strikes_Body Total Strikes_Landed float,
Round4_Strikes_Clinch Body Strikes_Attempts float,
Round4_Strikes_Clinch Body Strikes_Landed float,
Round4_Strikes_Clinch Head Strikes_Attempts float,
Round5_Grappling_Reversals_Landed float,
Round5_Grappling_Standups_Landed float,
Round5_Grappling_Submissions_Attempts float,
Round5_Grappling_Takedowns_Attempts float
Round5_Grappling_Takedowns_Landed float,
Round5_Strikes_Body Significant Strikes_Attempts float,
Round5_Strikes_Body Significant Strikes_Landed float,
Round5_Strikes_Body Total Strikes_Attempts float,
Round5_Strikes_Body Total Strikes_Landed float,
Round5_Strikes_Clinch Body Strikes_Attempts float,
Round5_Strikes_Clinch Body Strikes_Landed float,
Round5_Strikes_Clinch Head Strikes_Attempts float,
PRIMARY KEY (ID, Fight_ID)
);

CREATE TABLE info_fighter_statistics_grappling (
ID int,
Fight_ID int,
winner text,
corner text,
winby text,
Last_round int,
Max_round int,
Prev int,
RStreak int,
Round1_Grappling_Reversals_Landed float,
Round1_Grappling_Standups_Landed float,
Round1_Grappling_Submissions_Attempts float,
Round1_Grappling_Takedowns_Attempts float
Round1_Grappling_Takedowns_Landed float,
Round2_Grappling_Reversals_Landed float,
Round2_Grappling_Standups_Landed float,
Round2_Grappling_Submissions_Attempts float,
Round2_Grappling_Takedowns_Attempts float
