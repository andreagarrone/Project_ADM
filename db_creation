USE ks_user22;

CREATE TABLE info_card (
Event_ID int PRIMARY KEY,
Fight_ID int,
Date date,
Name text,
ID int,
corner text
);

CREATE TABLE info_fight (
Fight_ID int,
ID int,
Event_ID int,
Date date,
winner text,
winby text,
corner text,
Last_round int,
Max_round int,
RStreak int,
Height int,
Weight int,
Age int,
HomeTown text,
PRIMARY KEY (Fight_ID, ID)
);

CREATE TABLE info_fighter_general (
ID int,
Age int,
Date int, // RIVEDI TIPO
Height int,
Weight int,
Prev int,
RStreak int,
HomeTown text,
Name text,
PRIMARY KEY (ID, Date, Age)
);

CREATE TABLE info_fighter_statistics_strikes (
ID int,
Fight_ID int,
winner text,
corner text,
winby text,
Last_round int,
Max_round int,
Prev int,
RStreak int,
Round1_Grappling_Reversals_Landed float,
Round1_Grappling_Standups_Landed float,
Round1_Grappling_Submissions_Attempts float,
Round1_Grappling_Takedowns_Attempts float
